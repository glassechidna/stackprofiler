<h4><%= "#{@file}:#{@first_line}" %></h4>

<table class="table table-striped table-condensed" style="font-family: monospace;">
  <tr>
    <td>
      <pre>
        <% max = @output.map {|r| r[:samples].length }.compact.max %>
        <% @output.each_with_index do |line, idx| %>
<%= sprintf('% *s', max, line[:samples]) if line[:samples].present? %>
<% end %></pre>
    </td>
    <td>
      <pre>
        <% @output.each do |line| %>
<%= line[:code] -%>
        <% end %>
      </pre>
    </td>
  </tr>
</table>
